{
    "name": "automaticko/currency-exchange-rate",
    "description": "Currency exchange rate",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Automaticko\\CurrencyExchangeRate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        "Automaticko\\CurrencyExchangeRate\\Tests\\": "tests"
      }
    },
    "authors": [
        {
            "name": "Juan Jos√© Coello Cabrera",
            "email": "automaticko@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^8.9",
        "phpunit/phpunit": "^10.3",
        "mohammedmanssour/form-request-tester": "^1.1",
        "nunomaduro/larastan": "^2.6",
        "nunomaduro/phpinsights": "^2.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Automaticko\\CurrencyExchangeRate\\CurrencyExchangeRateServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "*",
        "guzzlehttp/guzzle": "^7.7",
        "ext-simplexml": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
